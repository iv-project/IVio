{
  "format_version": "1",
  "packages": [
    {
      "name": "zlib",
      "version": "1.3.1",
      "url": "https://zlib.net/zlib-1.3.1.tar.gz",
      "options": [
        "ZLIB_BUILD_EXAMPLES OFF"
      ],
      "cmake_commands": [
        "add_library(zlib::zlib ALIAS zlib)"
      ]
    },
    {
      "if": "PROJECT_IS_TOP_LEVEL",
      "name": "fmt",
      "version": "11.0.2",
      "github_repository": "fmtlib/fmt",
      "git_tag": "{VERSION}"
    },
    {
      "if": "PROJECT_IS_TOP_LEVEL",
      "name": "Catch2",
      "version": "3.7.1",
      "github_repository": "catchorg/Catch2"
    }
  ]
}
